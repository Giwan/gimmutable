{
  "name": "Gemplate",
  "private": true,
  "version": "1.0.0",
  "description": "React Template created by Giwan Persaud",
  "main": "index.html",
  "config": {
    "port": "8080"
  },
  "scripts": {
    "clean": "rm -rf dist/* ",
    "test": "jest",
    "start-server": "json-server --watch src/api/db.json --port 3001",
    "prestart": "webpack",
    "start": "webpack-dev-server --hot --inline --colors"
  },
  "keywords": [
    "news",
    "reader",
    "language"
  ],
  "author": "Giwan Persaud",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^5.8.25",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "core-js": "^1.2.1",
    "css-loader": "^0.19.0",
    "eslint": "^1.6.0",
    "eslint-loader": "^1.1.0",
    "fbjs": "^0.5.1",
    "file-loader": "^0.8.4",
    "intl": "^1.0.0",
    "jest-cli": "^0.8.2",
    "jest-webpack-alias": "^2.0.0",
    "node-sass": "^3.3.3",
    "postcss-loader": "^0.6.0",
    "react-addons-test-utils": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.4",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "flux": "^3.1.2",
    "immutable": "^3.8.1",
    "json-server": "^0.10.0",
    "keymirror": "^0.1.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "testFileExtensions": [
      "es6",
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6",
      "jsx"
    ]
  }
}
